{
  "name": "voicemaker",
  "version": "1.1.1",
  "description": "Make your nodeJs speak",
  "main": "dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "typings": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },

  "scripts": {
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean",
    "build": "tsc --pretty --project . && tsc --pretty --project tsconfig-cjs.json",
    "prepare": "npm run build",
    "dumpVoicesGoogle": "ts-node --project tsconfig-cjs.json src/googleTTS2/dumpVoices.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "voicemaker": "./bin/voicemaker"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UrielCh/voicemaker.git"
  },
  "keywords": [
    "elevenlabs",
    "voicemaker",
    "voicemaker.in",
    "translate.google.com",
    "google-tts",
    "cloud.ibm.com",
    "watson",
    "text-to-speech",
    "tts"
  ],
  "author": "Uriel Chemouni <uchemouni@gmail.com> (https://urielch.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UrielCh/voicemaker/issues"
  },
  "homepage": "https://github.com/UrielCh/voicemaker#readme",
  "devDependencies": {
    "@types/node": "^20.1.1",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@google-cloud/text-to-speech": "^4.2.2",
    "axios": "^0.22.0",
    "commander": "^8.2.0",
    "google-tts-api": "^2.0.2"
  },
  "files": [
    "dist/",
    "bin/",
    "CHANGELOG.md"
  ]
}
